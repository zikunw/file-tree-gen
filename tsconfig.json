      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "sourceMap": true,
      "outDir": "dist",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "*": ["src/*"]
